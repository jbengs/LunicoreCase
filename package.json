{
    "name": "LunicoreCase",
    "version": "1.0.0",
    "description": "App for car sales built with React, Express and SQLite.",
    "private": false,
    "license": "MIT",
    "browserslist": [
      "last 7 versions",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "main": "src/index.tsx",
    "prox": "http://localhost:4001",
    "scripts": {
      "build": "node ./buildScript",
      "build-front": "react-scripts build",
      "eject": "react-scripts eject",
      "start-server": "nodemon server/server.js --watch server/*",
      "start-front": "react-scripts start",
      "start": "concurrently \"npm run start-server\" \"npm run start-front\" --kill-others --kill-others-on-fail",
      "test": "react-scripts test --env=jsdom"
    },
    "dependencies": {
      "axios": "0.27.2",
      "body-parser": "1.20.0",
      "compression": "1.7.4",
      "cors": "2.8.5",
      "express": "4.18.1",
      "helmet": "5.1.0",
      "knex": "2.1.0",
      "react": "18.1.0",
      "react-dom": "18.1.0"
    },
    "devDependencies": {
      "@types/express": "4.17.13",
      "@types/react": "18.0.12",
      "@types/react-dom": "18.0.5",
      "concurrently": "7.2.1",
      "nodemon": "2.0.16",
      "react-scripts": "5.0.1",
      "sqlite3": "5.0.8",
      "typescript": "4.7.3"
    }
}